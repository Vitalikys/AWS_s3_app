[loggers]
keys=root,sampleLogger,vitalik01

[handlers]
keys=file_handler,vitHand

[formatters]
keys=sampleFormatter,vitalFormatter

[logger_vitalik01]
level=INFO
handlers=vitHand

[logger_root]
level=INFO
handlers=file_handler

[logger_sampleLogger]
level=INFO
handlers=fileHandler
qualname=sampleLogger
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=INFO
formatter=sampleFormatter
args=(sys.stdout,)

[handler_vitHand]
class=FileHandler
level=INFO
formatter=vitalFormatter
args=('logs/vit_s3_app.log', 'a', 'maxBytes=3255780', 'backupCount=15')

[formatter_sampleFormatter]
format=F1 %(asctime)s %(levelname)s %(message)s
style=%
validate=True
class=logging.Formatter

[formatter_vitalFormatter]
format=F1 %(asctime)s %(levelname)s %(message)s
style=%
validate=True
class=logging.Formatter

[handler_file_handler]
class=FileHandler
level=INFO
args=('logs/app.log', 'a', 'maxBytes=3255780', 'backupCount=15')
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s